<h1>写真アップロード</h1>

<%= form_with(model: @image, local: true) do |form| %>
  <div>
    <%= form.label :title, "タイトル" %><br>
    <%= form.text_field :title %>
  </div>

  <div>
    <%= form.label :image, "画像を選択" %><br>
    <%= form.file_field :image %>
  </div>

  <div>
    <%= form.submit "アップロード" %>
  </div>
<% end %>

<%= link_to "アップロード済み画像一覧", user_images_path %>
